{
  "name": "api-hexa-base",
  "version": "1.0.0",
  "description": "API con arquitectura hexagonal en JavaScript",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "dev": "node --watch main.js",
    "migrate": "npx sequelize-cli db:migrate --config src/infrastructure/db/sequelize/migrations/config.cjs",
    "seed": "npx sequelize-cli db:seed:all --config src/infrastructure/db/sequelize/migrations/config.cjs"
  },
  "keywords": [
    "hexagonal",
    "clean-architecture",
    "nodejs"
  ],
  "author": "Gonzalo Gonzalez",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.14.3",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.3"
  }
}
